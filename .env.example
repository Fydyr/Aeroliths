# Application name (for Docker containers)
NAME=aeroliths

# PostgreSQL Database (Prisma)
POSTGRES_USER=aeroliths_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=aeroliths
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@localhost:5433/${POSTGRES_DB}?schema=public

# ArangoDB Database (Graph)
ARANGO_URL="http://localhost:8529"
ARANGO_DB="_system"
ARANGO_USER="root"
ARANGO_PASSWORD="your_arango_password_here"
